<templateSet group="AndroidLog">
  <template name="loge" value="android.util.Log.e(&quot;ft:debug&quot;, &quot;$METHOD_NAME$&quot;);" description="Log.e(TAG, String, Exception)" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <context />
  </template>
  <template name="logi" value="android.util.Log.i(&quot;ft:debug&quot;, &quot;$METHOD_NAME$&quot;);" description="Log.i(TAG, String)" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <context />
  </template>
</templateSet>